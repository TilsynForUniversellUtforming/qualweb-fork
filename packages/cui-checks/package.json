{
  "name": "@qualweb/cui-checks",
  "version": "0.1.0",
  "description": "Accessibility tests for conversational user interfaces",
  "main": "dist/cui.bundle.js",
  "scripts": {
    "tsc": "tsc",
    "test": "mocha",
    "validate:rules": "node validate-rules.js",
    "compile:rules": "node compile-rules.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier src/**/*.ts --write",
    "prebuild": "npm run validate:rules && npm run compile:rules && rimraf prebuild && tsc --build tsconfig.prod.json",
    "build": "npm run prebuild && rimraf dist && webpack --mode production && rimraf prebuild",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/dom": "0.2.10",
    "@qualweb/locale": "0.1.18",
    "@qualweb/qw-page": "0.2.22",
    "@qualweb/types": "0.7.26",
    "@qualweb/util": "0.5.41",
    "@tsconfig/recommended": "^1.0.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.6.11",
    "ajv": "^6.12.6",
    "chai": "^4.4.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "node-fetch": "^2.6.7",
    "prettier": "^3.1.1",
    "puppeteer": "^21.6.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.7.4",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@qualweb/cui-checks": "^0.1.0"
  }
}
