{
  "name": "@qualweb/util",
  "version": "0.5.41",
  "description": "Utilities module for qualweb",
  "main": "dist/__webpack/util.bundle.js",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/util.bundle.js",
      "require": "./dist/util.bundle.js",
      "types": "./dist/index.d.ts"
    },
    "./applicability": {
      "import": "./dist/applicability/applicability.js",
      "require": "./dist/applicability/applicability.js",
      "types": "./dist/applicability/applicability.d.ts"
    },
    "./accessibilityUtils": {
      "import": "./dist/accessibilityUtils/accessibilityUtils.js",
      "require": "./dist/accessibilityUtils/accessibilityUtils.js",
      "types": "./dist/accessibilityUtils/accessibilityUtils.d.ts"
    },
    "./domUtils": {
      "import": "./dist/domUtils/domUtils.js",
      "require": "./dist/domUtils/domUtils.js",
      "types": "./dist/domUtils/domUtils.d.ts"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --spec=test/**/*.spec.ts",
    "test:ANameTest": "mocha --spec=test/ANameTest.spec.ts",
    "test:ANameTestWiki": "mocha --spec=test/ANameTestWiki.spec.ts",
    "test:svg": "mocha --spec=test/ANameSVGTest.spec.ts",
    "test:elementHidden": "mocha --spec=test/elementHidden.spec.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier src/**/*.ts --write",
    "prebuild": "rimraf prebuild && tsc --build tsconfig.prod.json",
    "build": "rimraf dist && npm run prebuild && webpack",
    "docs": "typedoc --out docs src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/qualweb/util#readme",
  "bugs": {
    "url": "https://github.com/qualweb/util/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/util.git"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "util",
    "lib"
  ],
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/core": "^0.7.43",
    "@qualweb/qw-page": "^0.2.19",
    "@tsconfig/recommended": "^1.0.3",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^15.6.1",
    "@types/string-pixel-width": "^1.10.3",
    "chai": "^4.4.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-select": "^5.1.0",
    "eslint": "^8.56.0",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "prettier": "^3.1.1",
    "puppeteer": "^22.12.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typescript": "5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "franc-min": "5.0.0",
    "string-pixel-width": "^1.10.0"
  }
}
